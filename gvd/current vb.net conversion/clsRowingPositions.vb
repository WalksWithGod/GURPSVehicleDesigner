	Public Function LocationCheck() As Boolean
		Dim TempCheck As Boolean
		'UPGRADE_WARNING: Couldn't resolve default property of object Veh.Components. Click for more: 'ms-help://MS.VSCC.v80/dv_commoner/local/redirect.htm?keyword="6A50421D-15FE-4896-8A1B-2EC21E9037B2"'
		If Veh.Components(Parent).Datatype = Body Then
			TempCheck = True
			'UPGRADE_WARNING: Couldn't resolve default property of object Veh.Components. Click for more: 'ms-help://MS.VSCC.v80/dv_commoner/local/redirect.htm?keyword="6A50421D-15FE-4896-8A1B-2EC21E9037B2"'
		ElseIf Veh.Components(Parent).Datatype = Pod Then 
			'UPGRADE_WARNING: Couldn't resolve default property of object Veh.Components. Click for more: 'ms-help://MS.VSCC.v80/dv_commoner/local/redirect.htm?keyword="6A50421D-15FE-4896-8A1B-2EC21E9037B2"'
			If Veh.Components(Veh.Components(Parent).LogicalParent).Datatype = Body Then
				TempCheck = True
			Else
				modHelper.InfoPrint(1, "Pods must be attached to Body in order to accept Rowing Positions.")
				TempCheck = False
			End If
		Else
			modHelper.InfoPrint(1, "Rowing positions must be placed in the Body or in Pods attached to hull.")
			TempCheck = False
		End If
		
		If TempCheck Then SetLogicalParent()
		LocationCheck = TempCheck
	End Function
	
	
