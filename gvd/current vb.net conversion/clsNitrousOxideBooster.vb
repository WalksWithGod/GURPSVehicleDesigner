	Public Function LocationCheck() As Boolean
		Dim TempCheck As Boolean
		Dim ParentType As Short
		
		'UPGRADE_WARNING: Couldn't resolve default property of object Veh.Components. Click for more: 'ms-help://MS.VSCC.v80/dv_commoner/local/redirect.htm?keyword="6A50421D-15FE-4896-8A1B-2EC21E9037B2"'
		ParentType = Veh.Components(Parent).Datatype
		
		'UPGRADE_WARNING: Couldn't resolve default property of object Veh.Components. Click for more: 'ms-help://MS.VSCC.v80/dv_commoner/local/redirect.htm?keyword="6A50421D-15FE-4896-8A1B-2EC21E9037B2"'
		If TypeOf Veh.Components(Parent) Is clsInternalCombustionEngine Then
			If ParentType = HydrogenCombustionEngine Then
				modHelper.InfoPrint(1, "Nitrous Oxide Boosters can be added to any Internal Combustion Engine except Hydrogen Combustion Engines.")
				TempCheck = False
			Else
				TempCheck = True
			End If
		Else
			modHelper.InfoPrint(1, "Nitrous Oxide Boosters can be added to any Internal Combustion Engine except Hydrogen Combustion Engines.")
			TempCheck = False
		End If
		
		If TempCheck Then SetLogicalParent()
		LocationCheck = TempCheck
	End Function
	
	
